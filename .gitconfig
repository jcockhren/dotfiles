[core]
        logallrefupdates = true
[user]
	email = $GIT_USER_EMAIL
	name = $GIT_USER_NAME
        signingkey = $GIT_SIGNING_KEYID
[alias]
	st = status
	ci = commit
	co = checkout
	cl = clone
	br = branch
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	m = merge
[push]
	default = current
[color]
	ui = true
[merge]
	ff = true
[commit]
        gpgsign = true
